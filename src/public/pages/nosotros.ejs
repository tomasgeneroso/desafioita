<%- include('../partials/head') %>
<%- include('../partials/nav') %>
<div class="row"style="background-color: #101054; color: white;" >
  <div class="container mt-3" >
    <div class="col-md-6 mt-5 mb-5" >
      <h1>Nosotros</h1>
      <h6>Te presentamos a ITA Team</h6>
      <button type="button" class="btn" style="border-radius: 20px; padding: 5px 25px 5px 25px; color: white; background-color:#cb4d94 ;">Contactanos</button>
    </div>
    
    <div class="col-md-6 mb-5" >
      <%#  <img class="img-fluid" style="display: block;"src="../resources/Recurso 403.png" alt="">%>
    </div>
  </div>
</div>
<div class="row justify-content-center pt-4 mb-5">
  <form action="/api/nosotros" method="POST" class=" justify-content-center pt-5" >
    <div class="form-row">
      <div class="form-group  col-md-offset-6  ">
        <label for="inputEmail4">First Name </label>
        <label for="inputEmail4"style="color: red;">*</label>
        <input type="text" name="firstName" class="form-control" id="inputEmail4" placeholder="First name">
      </div>
      <div class="form-group  col-md-offset-6 ">
        <label for="inputPassword4">Last Name</label>
        <label for="inputPassword4"style="color: red;">*</label>
        <input type="text" name="lastName" class="form-control" id="inputPassword4" placeholder="Last name">
      </div>
    </div>
    <div class="form-row">
      <div class="form-group  col-md-offset-6  ">
        <label for="inputEmail4">E-mail</label>
        <label for="inputEmail4" style="color: red;">*</label>
        <input type="email" name="email"class="form-control" id="inputEmail4" placeholder="E-mail">
      </div>
      <div class="form-group  col-md-offset-6 ">
        <label for="inputPassword4">Phone</label>
        <label for="inputPassword4"style="color: red;">*</label>
        <input type="text" name="phone" name="email"class="form-control" id="inputPassword4" placeholder="Phone">
      </div>
    </div>
    <div class="form-row">  
      <div class="form-group col-md-8">
        <label for="inputCity">City</label>
        <input type="text" name="city" class="form-control" id="inputCity" placeholder="City">
      </div>
    </div>
    <div class="form-row">  
      <div class="form-group  col-md-offset-6 ">
        <label for="inputCountry">Country</label>
        <input type="text" name="country" class="form-control" id="inputCountry">
      </div>
      <div class="form-group  col-md-offset-6 ">
        <label for="inputZip">Zip</label>
        <input type="text" name="zip" class="form-control" id="inputZip">
      </div>
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
    
  </form>

</div>
<%- include('../partials/footer') %>